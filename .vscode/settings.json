{
    "editor.rulers": [80],
    "editor.formatOnSave": true,
    "javascript.suggest.paths": false,
    "typescript.suggest.paths": false,
    "protoc": {
        "options": ["--proto_path=${workspaceRoot}/proto"]
    },
    "eslint.run": "onSave",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    }
}

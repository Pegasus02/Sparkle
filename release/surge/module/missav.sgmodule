#!name=MissAV去广告
#!desc=去广告，禁弹窗，禁止失焦自动暂停
#!category=Block

[Rule]
DOMAIN-SUFFIX,creative.myavlive.com,REJECT,pre-matching
DOMAIN-SUFFIX,trackwilltrk.com,REJECT,pre-matching
DOMAIN-SUFFIX,tsyndicate.com,REJECT,pre-matching

[Script]
missav.html = type=http-response,pattern=^https:\/\/missav\.(?:ai|ws)\/,requires-body=1,max-size=-1,engine=webview,script-path=https://raw.githubusercontent.com/kokoryh/Sparkle/master/dist/missav.html.js

[MITM]
hostname = %APPEND% missav.ai, missav.ws
